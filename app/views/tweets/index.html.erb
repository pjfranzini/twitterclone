<h1>Tweets index</h1>
<table class="table">
  <tr>
    <td>Tweet </td>
    <td>User </td>
    <td>Date tweeted </td>
  </tr>
  <% @tweets.each do |tweet| %>
  <tr>
    <td> <%= tweet.content %> </td>
    <td> <%= tweet.user_id %> </td>
    <td> <%= tweet.created_at %> </td>
  </tr>
  <% end %>
  <!-- loop over the collection tweets, displaying
the content field of each tweet -->
</table>
